{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/node" 
      },
      {
        "src": "backend/src/server/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "backend/src/server/routes/Principal.js"
      },
      {
        "src": "frontend/src/App.tsx",
        "dest": "/frontend/$1",
        "cleanUrls": true,
        "headers": {
          "Cache-Control": "s-maxage=1, stale-while-revalidate"
        }
      }
    ]
  }
  